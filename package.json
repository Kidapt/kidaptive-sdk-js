{
  "name": "kidaptive-sdk-js",
  "version": "1.0.0",
  "description": "Kidaptive ALP Javascript SDK",
  "main": "./dist/kidaptive_sdk.js",
  "scripts": {
    "install": "bower install",
    "build:irt": "cd src/js/main/irt && (make clean; make)",
    "build:debug": "cat $(cat build_order) | uglifyjs -b --wrap KidaptiveSdk > dist/kidaptive_sdk.js",
    "build": "npm run build:irt && npm run build:debug && uglifyjs -m < dist/kidaptive_sdk.js > dist/kidaptive_sdk.min.js",
    "version": "npm run build && git add -A",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kidapt/kidaptive-sdk-js.git"
  },
  "keywords": [
    "kidaptive",
    "javascript",
    "alp",
    "sdk"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kidapt/kidaptive-sdk-js/issues"
  },
  "homepage": "https://github.com/Kidapt/kidaptive-sdk-js#readme",
  "devDependencies": {
    "uglify-js": "^3.0.11",
    "watchify": "^3.9.0"
  }
}
